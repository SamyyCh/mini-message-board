<%- include('layout') %>
<h1><%= title %></h1>
<% messages.forEach(function(message, index) { %>
    <h3><%= message.user %></h3> 
    <p><%= message.text %></p> 
    <span><%= message.added %></span>
    <form action="/message/<%= index %>" method="get" style="display:inline;">
      <button type="submit">Open</button>
    </form>  
    <% }); %>
  <div>
    <a href="/new">Add a new message</a>
  </div>
  